      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="abstractCustomizableEntity" abstract="true">
    <xs:complexContent>
      <xs:extension base="abstractEntity">
        <xs:sequence>
          <xs:element name="customFieldMap">
            <xs:complexType>
