<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
       
	<bean id="emailSendingService" class="com.orangeleap.tangerine.service.communication.EmailService">
		<property name="uri" value="http://${jasper.host}:${jasper.port}/jasperserver/services/repository"/>
		<property name="userName" value="jasperadmin"/>
		<property name="password" value="jasperadmin"/>
	</bean>
</beans>